{
  "name": "ilhamsj/wordpress-meilisearch",
  "type": "wordpress-plugin",
  "description": "Wordpress plugin for meilisearch",
  "authors": [
    {
      "name": "ilhamsj",
      "email": "ilhamsaputrajati@gmail.com"
    }
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Hooks\\": "hooks/",
      "Utils\\": "utils/",
      "Constants\\": "constants/",
      "Services\\": "services/",
      "Admin\\": "admin/"
    }
  },
  "require": {
    "monolog/monolog": "^3.9",
    "meilisearch/meilisearch-php": "dev-main",
    "guzzlehttp/guzzle": "8.0.x-dev",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
